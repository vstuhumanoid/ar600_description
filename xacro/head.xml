<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="ar600">

	<xacro:macro name="torso" params="parent"> 
		<link name="torso">
	 		<xacro:wtf/>
	 	</link>

	 	<joint name="torso_r_joint"
	 		   type="revolute">
	 		<parent link="${parent}"/>
	 		<child link="torso"/>
	 		<!-- z unknown -->
	 		<origin xyz="0 0 0.1" rpy="0 0 0"/>
	 		<axis xyz="0 0 1"/>
	 		<limit lower="${-45*d2r}" upper="${45*d2r}" effort="10" velocity="10"/>
	 	</joint>

	</xacro:macro>

</robot>