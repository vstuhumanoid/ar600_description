<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="ar600">

	<xacro:macro name="torso" params="parent"> 
		<link name="torso">
	 		<xacro:meshes file="torso" collision="True"/>
	 		<inertial>
	 			<!-- origin is incorrect -->
	 			<origin xyz="0 0 -0.09" rpy="0 0 0"/>
      			<mass value="13.420"/>
      			<inertia ixx="0.141741" ixy="0.000000" ixz="0.000001"
      				                    iyy="0.178527"  iyz="0.003353"
      				                                    izz="0.107042"/>
    		</inertial>
	 	</link>

	 	<joint name="torso_r_joint" type="revolute">
	 		<parent link="${parent}"/>
	 		<child link="torso"/>
	 		<!-- z unknown -->
	 		<origin xyz="0 0 0.46" rpy="0 0 0"/>
	 		<axis xyz="0 0 1"/>
	 		<limit lower="${-45*d2r}" upper="${45*d2r}" effort="50" velocity="10"/>
	 	</joint>

	 	<xacro:transmission_block joint_name="torso_r_joint"/>

	</xacro:macro>
</robot>